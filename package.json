{
  "name": "srawn-editor",
  "version": "1.0.0",
  "description": "SR(A)WN Editor",
  "author": "Student Robotics",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "start": "node ./express-server.js",
    "lint": "eslint src/",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts registry.npmjs.org",
    "lint:spelling": "cspell --no-progress --unique",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss}": [
      "prettier --write --tab-width 2"
    ]
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.5.0",
    "@octokit/core": "^5.0.2",
    "@stackoverflow/stacks-editor": "^0.9.1",
    "@uiw/react-md-editor": "^4.0.1",
    "core-js": "3.34.0",
    "entities": "^4.5.0",
    "immer": "^10.0.3",
    "is-plain-object": "^5.0.0",
    "luxon": "3.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-router-dom": "6.21.2",
    "rehype-sanitize": "^6.0.0",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-transform-react-jsx": "7.23.4",
    "@babel/preset-env": "7.23.6",
    "@babel/preset-react": "7.23.3",
    "babel-loader": "9.1.3",
    "buffer": "6.0.3",
    "cross-env": "7.0.3",
    "cspell": "8.1.3",
    "css-loader": "6.8.1",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.6.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "lockfile-lint": "4.12.1",
    "mini-css-extract-plugin": "2.7.6",
    "path-browserify": "1.0.1",
    "prettier": "3.1.1",
    "sass": "1.69.5",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
